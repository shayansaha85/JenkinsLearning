pipeline {
    agent any
    stages {
        stage('Hello-FromGit') {
            steps {
                echo 'Hello World'
            }
        }
         stage('Build-FromGit') {
            steps {
                echo 'Building'
            }
        }
         stage('Deploy-FromGit') {
            steps {
                echo 'Deploying'
            }
        }
         stage('Test-FromGit') {
            steps {
                echo 'Testing'
            }
        }
         stage('Release-FromGit') {
            steps {
                echo 'Releasing'
            }
        }
    }
}
